{
    "name": "server",
    "version": "1.0.0",
    "engines": {
        "node": "10.18.1",
        "npm": "6.13.7"
    },
    "scripts": {
        "client": "cd client && npm start",
        "server": "nodemon server.js",
        "build": "cd client && npm run build",
        "dev": "concurrently --kill-others-on-fail \"npm run server\" \"npm run client\"",
        "start": "node server.js"
    },
    "dependencies": {
        "axios": "^0.21.1",
        "body-parser": "^1.18.3",
        "cors": "2.8.5",
        "currency-codes": "^2.1.0",
        "dotenv": "7.0.0",
        "express": "^4.16.4"
    },
    "devDependencies": {
        "concurrently": "^4.0.1",
        "nodemon": "^1.19.1"
    }
}
